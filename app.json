    },
    "android": {
      "package": "com.mastershifu24.thikhrapp",
      "versionCode": 1,
      "adaptiveIcon": {
        "backgroundColor": "#27ae60"
      }
    },
    "plugins": [
      "expo-notifications",
      "expo-asset"
    ],
    "extra": {
      "eas": {
        "projectId": "855a4cf9-249f-4f23-a05d-76fa7991f50c"
      }
    }
  }
}
